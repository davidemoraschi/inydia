body
{
	background-color: "#0B5eBB";
	background-image: url(../images/background.jpg);
	background-repeat: no-repeat;
}
.small_panel { height: 350px; width: 48%; float: left; margin-top: 1%; margin-left: 1% }
.progr_panel { height: 50px; width: 97%;  float: left; margin-top: 1%; margin-left: 1% }
.info_panel  { height: 150px; width: 97%; border: 1px solid; float: left; margin-top: 1%; margin-left: 1%; color:#FFFFFF; }

td
{
	position: relative;
}    

.headback
{
	filter: alpha(opacity=10);
	position: absolute;
	left: 205px;
	top: 300px;
	color: White;
	font-family: Arial;
	font-size: 150px;
	text-align: left;
	font-weight: bold;
}

.headfront
{
	position: absolute;
	top: 25px;
	left: 105px;
	color: White;
	font-family: Arial;
	font-size: 25px;
	text-align: left;
	font-weight: bolder;
}

.leftmenu
{
	position: absolute;
	top: 200;
	left: 20;
	color: White;
	font-family: Arial;
	font-size: 20pt;
	font-weight: bold;
	font-size: 20pt;
	text-align: center;
	padding-top: 10px;
}

.navmenu
{
	position: absolute;
	top: 100;
	left: 280;
	height: 63%;
	width: 72%;
	overflow: hidden;
}

.navlabel
{
	color: White;
	width: 150px;
	height: 50px;
	font-family: Arial;
	font-size: 20pt;
	font-weight: bold;
	text-align: center;
	padding-top: 10px;
}

.navbutton
{
	behavior:url(Hilite.htc);
	color: White;
	background-image: url(../images/navbutton.gif);
	background-repeat: no-repeat;
	width: 250px;
	height: 50px;
	font-family: Arial;
	font-size: 20pt;
	font-weight: bold;
	text-align: center;
	padding-top: 10px;
	cursor: hand;
}

.navbutton_hilite
{
	behavior: url(Hilite.htc);
	color: White;
	background-image: url(../images/navbutton_hilite.gif);
	background-repeat: no-repeat;
	width: 250px;
	height: 50px;
	font-family: Arial;
	font-size: 20pt;
	font-weight: bold;
	text-align: center;
	padding-top: 10px;
	cursor: hand;
}

.chart {position:relative; width:80%; background:#0B5eBB;}
.bar {position:absolute; left:0px; background:#FFFFFF; color:#FFF;}
